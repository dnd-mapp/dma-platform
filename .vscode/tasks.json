{
    "version": "2.0.0",
    "tasks": [
        // auth-client
        {
            "label": "auth-client:build",
            "type": "shell",
            "command": "pnpm nx build auth-client",
            "group": {
                "kind": "build"
            },
            "problemMatcher": ["$tsc"]
        },
        {
            "label": "auth-client:serve",
            "type": "shell",
            "command": "pnpm nx serve auth-client",
            "isBackground": true,
            "problemMatcher": ["$tsc-watch"]
        },
        {
            "label": "auth-client:test:ci",
            "type": "shell",
            "command": "pnpm nx test auth-client",
            "group": {
                "kind": "test"
            },
            "problemMatcher": {
                "owner": "vitest-ci",
                "fileLocation": ["relative", "${workspaceFolder}"],
                "pattern": {
                    "regexp": "^\\s*(?:✘|×)\\s+(.*):(\\d+):(\\d+)\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "message": 4
                }
            }
        },
        {
            "label": "auth-client:test:development",
            "type": "shell",
            "command": "pnpm nx test auth-client -c development",
            "group": {
                "kind": "test"
            },
            "isBackground": true,
            "problemMatcher": {
                "owner": "vitest",
                "fileLocation": ["relative", "${workspaceFolder}"],
                "pattern": {
                "regexp": "^\\s*(✘|×)\\s+(.*)\\s+(\\d+):(\\d+)$",
                "file": 2,
                "line": 3,
                "column": 4,
                "message": 2
              },
              "background": {
                "activeOnStart": true,
                "beginsPattern": ".*(RESTART|CHANGES).*|.*(RUN).*|.*(WAIT).*",
                "endsPattern": ".*(PASS|FAIL).*|.*(Tests).*|.*(waiting for changes).*"
              }
            },
        },
