{
    "name": "dnd-mapp-e2e",
    "$schema": "../../node_modules/nx/schemas/project-schema.json",
    "projectType": "application",
    "sourceRoot": "e2e/dnd-mapp/src",
    "implicitDependencies": ["dnd-mapp"],
    "targets": {
        "playwright-install": {
            "executor": "nx:run-commands",
            "options": {
                "command": "playwright install chromium firefox webkit"
            }
        },
        "e2e": {
            "executor": "nx:run-commands",
            "dependsOn": ["playwright-install"],
            "outputs": ["{workspaceRoot}/reports/{projectRoot}"],
            "cache": true,
            "options": {
                "command": "playwright test",
                "cwd": "{projectRoot}"
            }
        }
    }
}
